{
	"name": "Blank",
	"version": "1.0",
	"author": "...",
	"url": "https://github.com/TheNintendofan1209/Blank-Mediawiki",
	"descriptionmsg": "blank-desc",
	"namemsg": "skinname-blank",
	"license-name": "CC0-1.0",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"ValidSkinNames": {
		"blank": {
			"class": "SkinBlank",
			"args": [ {
				"name": "blank",
				"responsive": true,
				"templateDirectory": "skins/Blank/templates",
				"styles": [
					"skins.blank"
				],
				"messages": [
					"sitetitle",
					"search",
					"tagline",
					"navigation-heading"
				],
				"scripts": [
					"skins.blank.js"
				]
			} ]
		}
	},
	"MessagesDirs": {
		"Blank": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.blank": {
			"class": "ResourceLoaderSkinModule",
			"features": [
				"normalize",
				"elements",
				"content",
				"interface",
				"logo"
			],
			"styles": {
				"resources/screen-common.less": {
					"media": "screen"
				},
				"resources/screen-desktop.less": {
					"media": "screen and (min-width: 751px)"
				},
				"resources/screen-mobile.less": {
					"media": "screen and (max-width: 750px)"
				},
				"resources/print.css": {
					"media": "print"
				}
			}
		},
		"skins.blank.js": {
			"scripts": [
				"resources/main.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Blank"
	},
	"ResourceModuleSkinStyles": {
		"blank": {
			"+ext.echo.styles.badge": "resources/extensions/echo.styles.badge.less"
		}
	},
	"AutoloadClasses": {
		"SkinBlank": "includes/SkinBlank.php",
		"BlankTemplate": "includes/BlankTemplate.php"
	},
	"manifest_version": 1
}
